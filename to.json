{
  "from": [
    "build/runestones/libyaml.linux.x86_64",
    "build/runestones/libyaml.windows.x86_64",
    "build/runestones/libyaml.macos.x86_64"
  ],
  "to": {
    "language": "odin",
    "package": "yaml",
    "out": "yaml.odin",
    "trim_prefix": {
      "functions": "yaml_",
      "types": "yaml_",
      "constants": "YAML_"
    },
    "ignore_arch": true,
    "use_when_else": true,
    "no_build_tag": true
  }
}